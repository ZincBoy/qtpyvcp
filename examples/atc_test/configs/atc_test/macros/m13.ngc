o<m13> sub

; Move tool carousel CW until ATC zero
; ; Parameter #5170 is used to track the current tool pocket (persistently)
(print, o<M13> P#<p>)
M113 P0 Q1

M64 P3 ; Move Motor FWD
M66 P3 L1 Q20
 O120 if [#5399 LT 0]
      (abort, failed to home carousel)
      M65 P3; stop motor
 O120 endif
   #5170 = 1
M65 P3 ; Stop motor
#5171 = 1

M113 P1 Q0
o<m13> endsub [1]

M2
